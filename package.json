{
  "name": "musings-auth",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/musings-sphere/musings-auth.git",
  "author": "Francis Masha <francismasha96@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.0.0",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "start:dev": "node -r ts-node/register -r dotenv-safe/config src/index.ts",
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "postbuild": "cp -R ./src/assets ./dist",
    "prod": "node -r dotenv-safe/config dist/index.js",
    "cli": "node -r dotenv-safe/config -r ts-node/register src/cli.ts",
    "eslint": "eslint .",
    "eslint:fix": "eslint \"*/**/*.{js,ts}\" --fix",
    "format-staged": "pretty-quick --staged",
    "husky": "pretty-quick --staged && yarn prettier && yarn tslint",
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "tslint": "tsc --noEmit && eslint \"*/**/*.{js,ts}\" --quiet",
    "test:unit": "jest --no-cache  --detectOpenHandles --runInBand --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrations": "typeorm migration:generate -n Migration",
    "prepare": "husky install",
    "commit": "cz",
    "commit:signed": "cz -S",
    "test": "snyk test"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.5",
    "@nestjs/core": "^8.0.5",
    "@nestjs/microservices": "^8.0.5",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/testing": "^8.0.5",
    "argon2": "^0.28.2",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-safe": "^8.2.0",
    "errorhandler": "^1.5.1",
    "exit-hook": "^2.2.1",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "googleapis": "^83.0.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.0.1",
    "mongodb": "^4.0.1",
    "nestjs-command": "^2.0.1",
    "nodemailer": "^6.6.3",
    "passport": "^0.4.1",
    "passport-facebook-token": "^4.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-oauth": "^1.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "rxjs-compat": "^6.6.7",
    "superagent": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "twing": "^5.0.2",
    "typeorm": "^0.2.36",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/cls-hooked": "^4.3.3",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/errorhandler": "^1.5.0",
    "@types/eslint": "^7.28.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/luxon": "^1.27.1",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/passport-jwt": "^3.0.6",
    "@types/prettier": "^2.3.2",
    "@types/rimraf": "^3.0.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "snyk": "^1.674.0",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
